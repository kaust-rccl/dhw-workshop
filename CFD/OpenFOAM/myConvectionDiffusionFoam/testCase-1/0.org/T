FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      T;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 1 0 0 0];

internalField   uniform 0;

boundaryField
{   
    xmin
    {
        type            cyclic;
    }
    xmax
    {
        type            cyclic;
    }
    ymin
    {
        type            slip;
    }
    ymax
    {
        type            slip;
    }
    frontAndBack
    {
        type            empty;
    }
}

