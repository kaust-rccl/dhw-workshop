FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      setFieldsDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

defaultFieldValues
(
    volScalarFieldValue T 0
    //volVectorFieldValue U (0 0 0)
);

regions
(
    boxToCell
    {
      
        box (0.4 0.4 -1) (0.6 0.6 1);
        fieldValues
        (
                volScalarFieldValue T 1
        );
    }
);

